i\"]\n","\n","def letras(frase):\n","  lis = sorted(frase)\n","  lili = []\n","  for i in lis:\n","    lili.append(len(i))\n","  ret = [len(lis[0]),len(lis[len(lis)-1]),prom(lili)]\n","  return ret\n","\n","letras(frases)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"RK9w7s3PzfCE","executionInfo":{"status":"ok","timestamp":1711240137606,"user_tz":180,"elapsed":8,"user":{"displayName":"JOSE ENRIQUE MATUS TORO","userId":"04493849764423916285"}},"outputId":"81044242-a9a9-466b-c069-8f4a20c257ae"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["11\n","2.25\n"]},{"output_type":"execute_result","data":{"text/plain":["[3, 8, 5.0]"]},"metadata":{},"execution_count":41}]},{"cell_type":"code","source":["lista = [\"hola\",\"como\",34,4,22,False,\"jA\",44.3]\n","\n","def sumas(lista):\n","  suma = 0\n","  for i in lista:\n","    if type(i) == int:\n","      suma = suma + i\n","    if type(i) == float or type(i) == complex or type(i) == bool:\n","      suma = suma +int(i)\n","  return suma\n","\n","sumas(lista)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"r1WTV30X-7v7","executionInfo":{"status":"ok","timestamp":1711245579607,"user_tz":180,"elapsed":318,"user":{"displayName":"JOSE ENRIQUE MATUS TORO","userId":"04493849764423916285"}},"outputId":"56d4b1ec-7e73-4142-eae2-340d95b8281a"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["104"]},"metadata":{},"execution_count":57}]},{"cell_type":"code","source":["from pickle import FALSE\n","numero = 1234.5678\n","\n","def transfomar(numero,a,b):\n","  letra = str(numero)\n","  lista = []\n","  listaDelantera = []\n","  listaTrasera = []\n","  for i in letra:\n","    lista.append(i)\n","\n","  contador = 0\n","  for i in lista:\n","    if i == \".\":\n","      listaDelantera.append(\".\")\n","    if contador < a:\n","      listaDelantera.append(i)\n","    else:\n","      listaDelantera.append(0)\n","    contador = conta